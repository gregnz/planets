public class MinimapIcons
{
 
	private Transform minimapIcon;
	private Godot.Vector3 parentPosition;
 
	private void Start()
	{
		foreach (Transform child in transform)
		{
			if (child.CompareTag("minimapIcon"))
 
				minimapIcon = child;
		}
	}

 
	void LateUpdate()
	{
		minimapIcon.transform.rotation = Quaternion.identity;
		parentPosition = GetComponent<Transform>().position;
		minimapIcon.transform.position = parentPosition;
	}
}
